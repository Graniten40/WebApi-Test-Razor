{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Services": "Information",
        "AppWebApi.Controllers": "None",
        "DbRepos": "None"
      }
    },
    "InMemory": {
      "LogLevel": {
        "Services": "Information",
        "AppWebApi.Controllers": "Information",
        "DbRepos": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationSecrets": {
    "SecretStorage": "UserSecrets"
  },
  "DataService": {
    "WebApiBaseUri": "https://music.api.public.seido.se/"
  },
  "DatabaseConnections": {
    "DefaultDataUser": "dbo",
    "MigrationUser": "root",
    "UseDataSetWithTag": "sql-music.sqlserver.docker"
  }
}
